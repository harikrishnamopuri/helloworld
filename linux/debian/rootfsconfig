#ISO file name without .iso extention.
ISOFILE=debian-8.9.0-amd64-DVD-1

# Any hostname and domain names assigned from dhcp take precedence over
# values set here. However, setting the values still prevents the questions
# from being shown, even if values come from dhcp.
export  ROOTFS_HOSTNAME=OcNOS
export  ROOTFS_DOMAINNAME=hackerspackers.com

#change bridge name according to you
export ROOTFS_BRIDGE=br0


